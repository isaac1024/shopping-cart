<?xml version="1.0" encoding="utf-8" ?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping">
    <entity name="ShoppingCart\Order\Domain\Order" table="orders">
        <id name="orderId" column="id" type="order_id" />
        <embedded name="address" class="ShoppingCart\Order\Domain\Address" use-column-prefix="false" />
        <embedded name="status" class="ShoppingCart\Order\Domain\Status" use-column-prefix="false" />
        <embedded name="name" class="ShoppingCart\Order\Domain\Name" use-column-prefix="false" />
        <field name="cartId" column="cart_id" type="cart_id" />
        <embedded name="numberItems" class="ShoppingCart\Order\Domain\NumberItems" use-column-prefix="false" />
        <embedded name="totalAmount" class="ShoppingCart\Order\Domain\TotalAmount" use-column-prefix="false" />
        <field name="productItems" column="product_items" type="order_product_items" />
    </entity>
</doctrine-mapping>

<?xml version="1.0" encoding="utf-8" ?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping">
    <entity name="ShoppingCart\Query\Cart\Domain\Cart" table="carts">
        <id name="id" column="id" type="string" />
        <field name="numberItems" column="number_items" type="integer" />
        <field name="totalAmount" column="total_amount" type="integer" />
        <field name="productItems" column="product_items" type="query_cart_product_items" />
    </entity>
</doctrine-mapping>
